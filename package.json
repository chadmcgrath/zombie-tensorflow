{
	"name": "tf-zombies",
	"version": "1.0.0",
	"description": "AI Zombie Apocalypse Simulator - Watch humans learn to survive using reinforcement learning (PPO) with TensorFlow.js",
	"main": "main.js",
	"scripts": {
		"start": "electron --max-old-space-size=4096 main.js",
		"dev": "electron --max-old-space-size=4096 main.js",
		"test": "jest",
