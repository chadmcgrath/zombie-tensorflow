		"url": ""
	},
	"dependencies": {
		"electron": "^30.0.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.4.0",
		"eslint": "^9.4.0",
		"globals": "^15.4.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"canvas": "^2.11.2",
		"@babel/core": "^7.23.0",
		"@babel/preset-env": "^7.23.0",
		"babel-jest": "^29.7.0"
	},
	"engines": {
		"node": ">=16.0.0",
